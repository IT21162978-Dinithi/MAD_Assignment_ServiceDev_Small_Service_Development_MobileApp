<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".viewBookinglistActivity">


    <ScrollView
        android:layout_width="409dp"
        android:layout_height="729dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="300dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintLayout"
                    android:layout_width="407dp"
                    android:layout_height="252dp"
                    android:background="@drawable/top_background"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0">

                    <TextView
                        android:id="@+id/hello"
                        android:layout_width="99dp"
                        android:layout_height="43dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="60dp"
                        android:text="@string/hello"
                        android:textColor="#FFFFFFFF"

                        android:textSize="@dimen/hello"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/pro_name"
                        android:layout_width="227dp"
                        android:layout_height="86dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="8dp"
                        android:text="@string/pro_name"
                        android:textColor="#fedd06"
                        android:textSize="@dimen/profile_name"
                        android:textStyle="bold"
                        app:layout_constraintStart_toStartOf="@+id/hello"
                        app:layout_constraintTop_toBottomOf="@+id/hello" />

                    <ImageView
                        android:id="@+id/imageView9"
                        android:layout_width="114dp"
                        android:layout_height="128dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.979"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.427"
                        app:srcCompat="@drawable/ser1" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_marginStart="32dp"
                    android:layout_marginEnd="32dp"
                    android:background="@drawable/white_background_1"
                    android:elevation="3dp"
                    android:orientation="horizontal"
                    app:layout_constraintBottom_toBottomOf="@+id/constraintLayout"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
                    app:layout_constraintVertical_bias="0.0">

                    <TextView
                        android:id="@+id/pay"
                        android:layout_width="331dp"
                        android:layout_height="79dp"
                        android:layout_marginTop="20dp"
                        android:layout_weight="1"
                        android:text="@string/booking"
                        android:textAlignment="center"
                        android:textColor="#0B0909"
                        android:textSize="34sp"
                        android:textStyle="bold" />


                </LinearLayout>


            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="428dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <TextView
                    android:id="@+id/msg"
                    android:layout_width="347dp"
                    android:layout_height="31dp"
                    android:text="@string/new_msg"
                    android:textColor="#513434"
                    android:textSize="@dimen/pro_message"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.296"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.041" />

                <LinearLayout
                    android:id="@+id/linearLayout2"
                    android:layout_width="395dp"
                    android:layout_height="302dp"
                    android:orientation="vertical"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <LinearLayout
                        android:id="@+id/details1"
                        android:layout_width="407dp"
                        android:layout_height="122dp"
                        android:background="@drawable/white_logpanel"
                        android:orientation="vertical"
                        android:paddingRight="5dp"
                        tools:ignore="MissingConstraints">

                        <LinearLayout
                            android:id="@+id/btn_ready"
                            android:layout_width="349dp"
                            android:layout_height="87dp">

                            <ImageView
                                android:id="@+id/imageView13"
                                android:layout_width="80dp"
                                android:layout_height="75dp"
                                android:layout_weight="1"
                                app:srcCompat="@drawable/pro2" />

                            <Button
                                android:id="@+id/btn_acc"
                                android:layout_width="100dp"
                                android:layout_height="61dp"
                                android:layout_marginRight="15dp"
                                android:layout_weight="1"
                                android:text="@string/btn_ready" />

                            <Button
                                android:id="@+id/btn_call"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:backgroundTint="#EF3737"
                                android:text="@string/btn_call" />

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/yearno"
                        android:layout_width="407dp"
                        android:layout_height="135dp"
                        android:background="@drawable/white_logpanel"
                        android:orientation="vertical"
                        android:paddingRight="5dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.533"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.542"
                        tools:ignore="MissingConstraints">

                        <LinearLayout
                            android:layout_width="369dp"
                            android:layout_height="79dp"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageView17"
                                android:layout_width="40dp"
                                android:layout_height="match_parent"
                                android:layout_marginStart="16dp"
                                android:layout_marginLeft="16dp"
                                android:layout_weight="1"
                                app:layout_constraintStart_toStartOf="parent"
                                app:srcCompat="@drawable/pro5" />

                            <Button
                                android:id="@+id/btn_pending"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="15dp"
                                android:layout_weight="1"
                                android:text="@string/btn_pending" />

                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>

                <Button
                    android:id="@+id/btn_all"
                    android:layout_width="145dp"
                    android:layout_height="48dp"
                    android:layout_marginBottom="12dp"
                    android:backgroundTint="#2A2E29"
                    android:text="@string/btn_all"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/linearLayout2" />


            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>